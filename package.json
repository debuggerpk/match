{
  "name": "simplex",
  "version": "1.0.0",
  "description": "Simple Order Engine",
  "main": "index.js",
  "author": "Yousuf Jawwad",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "docs:html": "typedoc src/ --target ES6 --mode file --out docs",
    "test": "ava-ts"
  },
  "dependencies": {
    "cuid": "^2.1.1",
    "rxjs": "^6.2.1",
    "typescript-collections": "^1.3.2"
  },
  "devDependencies": {
    "@types/node": "^10.5.1",
    "ava": "^0.25.0",
    "ava-ts": "^0.24.6",
    "prettier": "^1.13.7",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  },
  "ava": {
    "files": [
      "src/**/*.spec.ts"
    ],
    "sources": [
      "src/**/*.ts"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "compileEnhancements": true,
    "require": [
      "ts-node/register"
    ]
  }
}
